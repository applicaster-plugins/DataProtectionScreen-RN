  "manifest_version": "0.1.0",
  "name": "DataProtectionScreen-RN",
  "description": "Plugin to display data protection user config and privacy text",
  "type": "general",
  "identifier": "DataProtectionScreen-RN",
  "whitelisted_account_ids": [
    "5c360f8a6b857000082d0e71",
